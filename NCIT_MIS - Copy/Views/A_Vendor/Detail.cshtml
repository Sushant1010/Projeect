@model NCIT_MIS.ViewModel.A_VendorVM

@{
    ViewBag.Title = "Vendor Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    <div class="layout-content-body container">
        <div class="title-bar" style="margin-bottom: -40px;">
            <div class="col-xs-10">  <h1 class="title-bar-title">Vendor Detail</h1> </div>

            <ol class="breadcrumb">
                <li class="list">@Html.ActionLink("Edit", "Edit", "A_Vendor", new { Id = Model.VendorId }, new { @class = "btn btn-xs-2 btn-success" })</li>
                <li class="list">@Html.ActionLink("List", "Index", null, new { @class = "btn btn-xs-2 btn-success" })</li>
            </ol>
        </div>
        <div class="row no-gutters">
            <div class="col-xs-12  ">
                <div class="card" style="margin-top: 30px;">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="demo-form-wrapper">
                                    @using (Html.BeginForm())
                                    {
                                        @Html.AntiForgeryToken()
                                    <div class="form form-horizontal">
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label" for="form-control-1">Vendor Name</label>
                                            <div class="col-sm-4">
                                                @Html.TextBox("CategoryName", Model.VendorName, new { @class = "form-control", ReadOnly = "ReadOnly" })
                                                @Html.ValidationMessageFor(model => model.VendorName, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label" for="form-control-1">Pan No.</label>
                                            <div class="col-sm-4">
                                                @Html.TextBox("CategoryName", Model.PanNo, new { @class = "form-control", ReadOnly = "ReadOnly" })
                                                @Html.ValidationMessageFor(model => model.PanNo, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label" for="form-control-1">Vendor Address</label>
                                            <div class="col-sm-4">
                                                @Html.TextBox("CategoryName", Model.VendorAddress, new { @class = "form-control", ReadOnly = "ReadOnly" })
                                                @Html.ValidationMessageFor(model => model.VendorAddress, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label" for="form-control-1">Vendor Phone No.</label>
                                            <div class="col-sm-4">
                                                @Html.TextBox("CategoryName", Model.VendorPhone, new { @class = "form-control", ReadOnly = "ReadOnly" })
                                                @Html.ValidationMessageFor(model => model.VendorPhone, "", new { @class = "text-danger" })
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-sm-2 control-label" for="form-control-1">Vendor Mobile No.</label>
                                            <div class="col-sm-4">
                                                @Html.TextBox("CategoryName", Model.VendorMobile, new { @class = "form-control", ReadOnly = "ReadOnly" })
                                                @Html.ValidationMessageFor(model => model.VendorMobile, "", new { @class = "text-danger" })
                                            </div>
                                        </div>

                                    </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>